{
    "name": "phossa/phossa-db",
    "license": "MIT",
    "type": "library",
    "description": "A PHP db libraray",
    "keywords": ["phossa", "db", "database", "rdbm"],
    "homepage": "https://github.com/phossa/phossa-db",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa"
        }
    ],
    "autoload": {
        "psr-0": { "Phossa\\Db\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Phossa\\Db\\": "tests/src/" }
    },
    "require": {
        "php": ">=5.4.0",
        "phossa/phossa-shared": "~1.0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml",  "composer.lock"]
    }
}
